                "firstname",
                "lastname",
                "password",
                "sex",
                "username"
            ],
            "properties": {
                "age": {
                    "type": "integer",
                    "maximum": 99,
                    "minimum": 18
                },
                "city": {
                    "type": "string",
                    "maxLength": 50
                },
                "country": {
                    "type": "string",
                    "maxLength": 50
                },
                "email": {
                    "type": "string",
                    "maxLength": 100
